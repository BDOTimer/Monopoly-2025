<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="sfml-Monopoly" />
		<Option pch_mode="2" />
		<Option compiler="gcc-14-ucrt-64" />
		<Build>
			<Target title="Release">
				<Option output="bin/sfml-Monopoly" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin/" />
				<Option object_output="obj/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DSFML_STATIC" />
					<Add directory="$(#sfml30)/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="sfml-graphics-s" />
					<Add library="sfml-window-s" />
					<Add library="sfml-system-s" />
					<Add library="libfreetype.a" />
					<Add library="libsfml-main-s.a" />
					<Add library="libgdi32.a" />
					<Add library="libwinmm.a" />
					<Add library="libopengl32.a" />
					<Add directory="$(#sfml30)/lib" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-DSFML_STATIC" />
			<Add directory="$(#sfml30)/include" />
		</Compiler>
		<Linker>
			<Add library="mingw32" />
			<Add library="user32" />
			<Add library="gdi32" />
			<Add library="winmm" />
			<Add library="dxguid" />
			<Add directory="$(#sfml30)/lib" />
		</Linker>
		<Unit filename="cb.bmp" />
		<Unit filename="icon.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="sources/common.h" />
		<Unit filename="sources/main.cpp" />
		<Unit filename="sources/model.h" />
		<Unit filename="sources/player.h" />
		<Unit filename="sources/render.h" />
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
